		the EAR's lib folder -->
	<deployment>
		<!-- Exclusions allow you to prevent the server from automatically adding 
			some dependencies -->
		<exclusions>
			<!-- module name="org.hibernate" / -->
		</exclusions>
	</deployment>
</jboss-deployment-structure>